public class Main {
    public static void main(String[] args) {
        Forma[] formas = new Forma[3];
        formas[0] = new Circulo(5);
        formas[1] = new Triangulo(3, 4);
        formas[2] = new Retangulo(6, 8);

        for (Forma forma : formas) {
            System.out.println("√Årea: " + forma.calcularArea());
        }
    }
}
